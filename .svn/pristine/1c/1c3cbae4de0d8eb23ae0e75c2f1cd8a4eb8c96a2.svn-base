* {
	padding: 0px;
	margin: 0px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

ul {
	list-style-type: none;
}

a {
	text-decoration: none;
}

body {
	font-weight: 400;
	font-family: "microsoft yahei", 'Open Sans', sans-serif;
	font-size: 15px;
	line-height: 1.42857143;
	color: #5e5e5e;
}

.card {
	position: relative;
	background: #fff;
	box-shadow: 0 1px 1px rgba(0, 0, 0, .15);
	margin-bottom: 30px;
}

.card-head {
	padding: 18px 25px;
	position: relative;
}

.card-body {
	padding: 15px 26px;
}

.from-1 {
	overflow: hidden !important;
	
}

.from-1-label {
	padding-top: 7px;
	margin-bottom: 0;
	text-align: right;
	width: 16.66666667%;
	min-height: 1px;
	padding-left: 15px;
	padding-right: 15px;
	display: inline-block;
	max-width: 100%;
}

.from-1-input {
	position: relative;
	margin: 14px 0px;
	min-height: 1px;
	padding-left: 15px;
	padding-right: 15px;
	display: inline-block;
	width: 500px;
}

.input {
	background-color: #FFFFFF;
	background-image: none;
	border: 1px solid #e5e6e7;
	border-radius: 1px;
	color: inherit;
	display: block;
	padding: 7px 12px;
	width: 100%;
	font-size: 14px;
}

.hr {
	width: 100%;
	height: 1px;
	border-bottom: 1px dashed #E0E0E0;
	margin: 5px 0px;
	clear: both;
}

.from-1_ul li{
   display: inline-block;
   height: 50px;
   line-height: 50px;
   margin: 0px 10px;
}

.from-1_ul li a:hover{
 color:rgb(46,204,113);
	font-weight: 400;
}
#divone{
	border: 1px solid #e7e7eb;
}
#divtwo{
	border: 1px solid #e7e7eb;
	cursor: pointer;
}
.divone_text{
	font-size: 16px;
min-height: 230px;
padding: 10px;
margin-top: 12px;
}
.divone_title{
	border: 1px solid #e7e7eb;
	height: 40px;
	line-height: 40px;
	text-align: right;
	padding-right: 8px;
}
#divtwo_1{
	
	margin: 15px;
	min-height: 230px;
	width: 450px;
}
#addpic{
	text-align: center;
	line-height: 230px;
	border: 1px  dashed #e7e7eb;
}
#addpic:hover{
	border: 1px  dashed #999999;
	cursor: pointer;
}
#divthree{
	border: 1px solid #e7e7eb;
	
}
